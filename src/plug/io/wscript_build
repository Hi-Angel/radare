#! /usr/bin/env python

obj = bld.create_obj('cc', 'shlib')
obj.env['CCFLAGS'].append('-I..')

obj.packages='gtk+-2.0'

obj.add_obj_file('gdb.o')
obj.add_obj_file('winedbg.o')
obj.add_obj_file('gdbx.o')
obj.add_obj_file('haret.o')
obj.add_obj_file('mmap.o')
obj.add_obj_file('posix.o')
obj.add_obj_file('remote.o')
obj.add_obj_file('shm.o')
obj.add_obj_file('socket.o')
